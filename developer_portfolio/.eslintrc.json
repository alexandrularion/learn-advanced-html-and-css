{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier","eslint:recommended"],
  "plugins": ["prettier","eslint-plugin-html"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "linebreak-style": "off",
    "quotes": ["warn", "double"],
    "prettier/prettier": ["error"]
  }
}
